{
  "name": "photoboth-vr-webapp",
  "version": "1.0.0",
  "description": "3D interactive photobooth with face and hand tracking",
  "type": "module",
  "main": "server/app.js",
  "scripts": {
    "start": "NODE_ENV=production node server/app.js",
    "dev": "NODE_ENV=development nodemon server/app.js",
    "build": "echo 'No build process needed for vanilla JS'",
    "docker:build": "docker build -t photoboth-vr .",
    "docker:run": "docker run -p 7000:7000 photoboth-vr"
  },
  "keywords": [
    "three.js",
    "p5.js",
    "mediapipe",
    "computer-vision",
    "photobooth",
    "3d",
    "web-app"
  ],
  "author": "Artist-Creator",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}